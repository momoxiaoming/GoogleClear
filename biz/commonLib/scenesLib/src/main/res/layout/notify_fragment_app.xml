<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <com.mckj.sceneslib.ui.scenes.model.tools.newtools.ui.weight.NewTitleBar
            android:id="@+id/notify_app_new_titleBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/scenes_textview5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#E4F0FF"
            android:paddingVertical="13dp"
            android:paddingLeft="13dp"
            android:text="检测通知的应用"
            android:textColor="#333333"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/notify_app_new_titleBar" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/notifyAppContainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/scenes_textview5" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>