<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/clean_item_root"
        android:layout_width="match_parent"
        android:layout_marginBottom="13dp"
        android:layout_height="90dp">


        <ImageView
            android:id="@+id/clean_item_content_iv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_width="90dp"
            android:layout_height="90dp"/>


        <ImageView
            android:id="@+id/clean_item_select_cb"
            app:layout_constraintTop_toTopOf="@id/clean_item_content_iv"
            app:layout_constraintRight_toRightOf="@id/clean_item_content_iv"
            android:layout_marginTop="4dp"
            android:layout_marginRight="4dp"
            android:tint="?colorPrimary"
            tools:tint="#000000"
            android:src="@drawable/cleanupx_gallery_select_sel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
