<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="OpenTheme">
        <!--app icon-->
        <attr name="OpenAppIcon" format="reference" />
        <!--app name-->
        <attr name="OpenAppName" format="reference|string" />
        <!--白色按钮点击-->
        <attr name="OpenSelectorWhiteBtn" format="reference|color" />
        <!--主题背景色-->
        <attr name="OpenPrimaryBackground" format="reference|color" />
        <!--主题按钮色-->
        <attr name="OpenPrimaryBtn" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="BiTileLayout" tools:ignore="ResourceName">

        <attr name="text1" format="reference|string" />
        <attr name="text2" format="reference|string" />

        <attr name="textColor1" format="reference|color" />
        <attr name="textColor2" format="reference|color" />

        <attr name="textSize1" format="reference|dimension" />
        <attr name="textSize2" format="reference|dimension" />

    </declare-styleable>

</resources>